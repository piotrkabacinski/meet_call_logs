{
  "name": "Google Meet Timer Logs",
  "version": "0.0.1",
  "manifest_version": 3,
  "minimum_chrome_version": "110.0",
  "description": "Google Meet calls end times logs",
  "permissions": ["activeTab", "scripting", "storage"],
  "action": {
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "js": ["content.js"],
      "matches": [
        "https://meet.google.com/*"
      ]
    }
  ]
}
